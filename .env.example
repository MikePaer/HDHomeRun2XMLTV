# HDHomeRun Configuration
HDHOMERUN_HOST=192.168.1.100

# Server Configuration
WEB_PORT=8083
NODE_ENV=development

# EPG Configuration
CRON_SCHEDULE=0 3 * * *
DAYS=7
HOURS_INCREMENT=3

# Timezone
TZ=America/Chicago

# Feature Flags
RUN_ON_START=true
ENABLE_DUMMY_PROGRAMMING=true

# Dummy Programming Configuration (only used if ENABLE_DUMMY_PROGRAMMING=true)
DUMMY_PROGRAM_TITLE=No Information
DUMMY_PROGRAM_DESC=No program information is currently available for {channel}.

# File Paths
OUTPUT_DIR=./output
EPG_FILENAME=epg.xml
